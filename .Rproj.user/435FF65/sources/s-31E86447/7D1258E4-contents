# This script reads in the data from flavors_of_cacao.csv
# This script takes no arguments.

# Usage: Rscript scripts/chocolate.R

# define main function
main <- function() {
  input <- "Data/flavors_of_cacao.csv"
  
  # read in data
  data <- read.csv(input)
  
  # print out the first few rows
  print(head(data))
}

# call main function
main()